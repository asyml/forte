      "attributes": [
        {
          "name": "label",
          "type": "str"
        },
        {
          "name": "sentiment",
          "type": "Dict",
          "key_type": "str",
          "value_type": "float"
        },
        {
          "name": "is_root",
          "type": "bool"
        },
        {
          "name": "is_leaf",
          "type": "bool"
        },
        {
          "name": "parent_node",
          "type": "ft.onto.base_ontology.ConstituentNode"
        },
        {
          "name": "children_nodes",
          "type": "List",
          "item_type": "ft.onto.base_ontology.ConstituentNode"
